    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run iTDP",
            "type": "cppdbg",
            "request": "launch",
            "program": "/root/iTDP0.2/build/iTDPlacer",
            "args": [
                "benchmark/ICCAD15.parm",
                "benchmark/superblue18/superblue18.iccad2015"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build iTDP",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}